/* 他人聊天 */

.list1 {
  /* display: flex; */
  margin-bottom: 15px;
}
.fl{
  display: flex; 
}

.list1 image {
  width: 30px;
  height: 30px;
  border-radius: 50%;
}

.content {
  max-width: 75%;
  margin-left: 10px;
  color: #8e98a1;
  font-size: 12px;
}

.desc {
  border: 1px solid #f6296c;
  background: #fff;
  border-radius: 5px;
  margin-top: 5px;
  padding: 5px 10px 5px 10px;
  position: relative;
  line-height: 34rpx;
}
.desc::before{
  position: absolute;
  left: -16px;
  top: 2px;
  z-index: 2;
  content: '';
  border: 8px solid transparent;
  transform-origin:center center;
  border-right: 8px solid #fff;
  transform:scale(1,0.5);
  width:0px;
  height:0px;
}
.desc::after{
  position: absolute;
  z-index: 1;
  top: -1.5px;
  left: -22px;
  content: '';
  border: 11px solid transparent;
  border-right: 11px solid #f6296c;
  transform-origin:center center;
  transform:scale(1,0.5);
  width:0px;
  height:0px;
}
.bg::before{
  position: absolute;
  left: -12px;
  top: 2px;
  z-index: 2;
  content: '';
  border: 6px solid transparent;
  transform-origin:center center;
  border-right: 6px solid #1f040f;
  transform:scale(1,0.5);
  width:0px;
  height:0px;
}
.bg::after{
  position: absolute;
  z-index: 1;
  top: -1.5px;
  left: -18px;
  content: '';
  border: 9px solid transparent;
  border-right: 9px solid #f6296c;
  transform-origin:center center;
  transform:scale(1,0.5);
  width:0px;
  height:0px;
}
