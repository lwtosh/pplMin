page{
  background: #f5f5f5;
}
.list{
  padding: 25rpx;
  background: #fff;
  margin-bottom: 10rpx;
}
.desc-name{
  color: #404040;
  font-size: 15px;
  font-family:PingFang SC;
  font-weight:500;
}
.desc-tel{
  font-size: 15px;
  color: #999999;
