   font-weight:400;
}
.desc-adderss{
  font-size: 14px;
  margin-top: 15rpx;
  color: #404040;
  line-height: 1.5;
  font-weight:400;
}
.is-details{
  display: flex;
  justify-content: space-between;
  margin-top: 10rpx;
  line-height: 50rpx;
}
.btn{
  position: fixed;
  z-index: 999;
  bottom: 0;
  width: 100%;
  line-height: 2.5;
  text-align: center;
  font-size: 17px;
  margin-top: -10px;
  background: linear-gradient(90deg, rgba(244, 81, 85, 1) 0%, rgba(245, 30, 115, 1) 100%);
  color: #fff;
  border-radius: 0px;
}
.i-cell{
  padding: 0 !important;
  line-height: 0 !important;
}
.del{
  padding: 8rpx 30rpx;
  border-radius: 8rpx;
  border: 1px solid #999;
  color: #999;
  font-size: 24rpx;
  margin-left: 30rpx;
}
.details{
  display: inline-block;
  color: #fff;
  font-size: 12px;
  width:75rpx;
  text-align: center;
   line-height:30rpx;
  background:linear-gradient(90deg,rgba(244,81,85,1) 0%,rgba(245,30,115,1) 100%);
  border-radius:20rpx;
  margin-right: 10rpx;
}
.set{
  font-size: 24rpx;
  color: #404040;
}

radio .wx-radio-input{ 
    height: 31rpx; 
    width: 31rpx;
    border-radius: 50%;
    border: 2rpx solid #999;
    background: transparent;
 }
 radio .wx-radio-input.wx-radio-input-checked::before{
   border-radius: 50%;/* 圆角 */
   width: 31rpx; /* 选中后对勾大小，不要超过背景的尺寸 */
   height: 31rpx; /* 选中后对勾大小，不要超过背景的尺寸 */
   line-height: 35rpx;
   text-align: center;
   font-size:25rpx; /* 对勾大小 30rpx */
   color:#fff; /* 对勾颜色 白色 */
   background: #f00;
   transform:translate(-50%, -50%) scale(1);
   -webkit-transform:translate(-50%, -50%) scale(1);
}
.desc{
  padding-bottom: 20rpx;
  border-bottom: 1px solid #f5f5f5
}