page{
  background: #F5F5F5;
  width: 100%;
  height: 100%;
}

.scoll-h {
  height: 100%;
}
.order-li{
  padding:10rpx 20rpx;
  background: #fff;
  margin-bottom: 15rpx;
}
.orderNum,.seletion1{
  display: flex;
  justify-content: space-between;
  line-height: 2.5;
}
.orderNum-left{
  color: #999;
  font-size: 24rpx;
}
.orderNum-right{
  font-size: 26rpx;
  color: #F52A6C;
}
.null{
  text-align: center;
  margin: 60rpx auto 0;
  width: 100%;
  height: 380rpx;
}
.null image{
  width: 80%;
  height: 100%;
}
.null-desc{
  font-size: 22rpx;
  color: #999;
  margin-top: 30rpx;
}
.seletion {
  background: #fff;
  padding: 10rpx 0rpx 20rpx 0rpx;
  display: flex;
  border-bottom: 1rpx solid #F2F2F2;
}
.seletion-left {
  width:196rpx;
  height: 196rpx;
  margin-right: 15rpx;
}
.seletion-img{
  width:196rpx;
  height: 196rpx;
  border-radius: 10rpx;
}
.seletion-img image {
  width: 100%;
  height: 100%;
  border-radius: 10rpx;
}
.left-bd {
  padding: 20rpx 16rpx 22rpx;
  background: #fff;
  position: relative;
  flex-grow: 1
}
.title {
  font-family: PingFang-SC-Regular;
  font-size: 30rpx;
  line-clamp: 10px;
  color: #404040;
  overflow: hidden; 
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical; 
  margin-bottom: 10rpx;
}
.num {
  color: #f52a6a;
  font-size: 32rpx;
  margin-top: 40rpx;
}
.mon{
  color: #f52a6a;
  font-size: 24rpx;
}
.tet{
  color: #808080;
  font-size: 24rpx;
}
.line{
  text-decoration: line-through;
}
.one{
  position: absolute;
  font-size: 20rpx;
  color: #F52A6C;
  right: 0rpx;
  top:35%;
}
.seletion1{
  width: 100%;
 line-height: 73rpx;
 font-size: 22rpx;
 color: #999999;
}
.box{
  width: 100%;
  display: flex;
  justify-content: space-between;
}

.book,.confirm{
  display: inline-block;
  border:1px solid #999;
  border-radius: 28rpx;
  width:130rpx;
  height:50rpx;
  line-height: 50rpx;
  text-align: center;
  font-size: 24rpx;
  color: #404040;
}
.confirm{
  border:none;
  background:linear-gradient(90deg,rgba(244,81,85,1) 0%,rgba(245,30,115,1) 100%);
  color: #fff;
  margin-left: 20rpx;
}
.chu{
  line-height: 1.8
}
.time1{
  margin-top: 15rpx;
}